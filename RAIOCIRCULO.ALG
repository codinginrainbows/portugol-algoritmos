Algoritmo "raiocirculo"



Var
          raio, area, pipi : real


Inicio

          escreva ("Digite o valor do raio do circulo: ")
          leia (raio)

          pipi <- 3.14159

          area <- pipi * (raio * raio)
          
          escreval
          
          escreva ("AREA = ", area:8:3)



Fimalgoritmo