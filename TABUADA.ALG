Algoritmo "tabuada"

Var

   n, i, resultado : inteiro

Inicio

      escreva ("digite um numero: ")
      leia (n)

      escreval
      enquanto i <= 9 faca
      
      i <- i + 1
      
      resultado <- i * n

      escreval(i, " x", n, " =", resultado)

      fimenquanto
      
      escreval
      escreva ("FIM")
      


Fimalgoritmo